{
   "name": "AWS",
   "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/aws.svg",
   "description": "",
   "hint": "Select the environment where you want to deploy Logz.io Telemetry Collector.",
   "subtypes": [
     {
       "name": "aws-cloudformation-cloudwatch",
       "label": "AWS Cloudformation (cloudwatch)",
       "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/aws_ec2_linux.png",
       "description": "S3 Linux server",
       "hint": "S3 with Linux.",
       "supportedOs": ["AWS_Cloudformation"],
       "integrationTemplate": "cloudwatch-shipper-trigger/1.0.0/sam-template.yaml",
       "datasources": [
         {
           "name": "System",
           "logo": "https://dytvr9ot2sszz.cloudfront.net/logz-docs/shipper-logos/local_host.png",
           "description": "Logz.io Uses OpenTelemetry to monitor your EC2.",
           "params": [
            {
              "type": "single-select",
              "name": "awsRegion",
              "label": "AWS Region",
              "description": "AWS Region",
              "required": true,
              "options": [
                { "label": "us-east-1", "value": "us-east-1" },
                { "label": "us-east-2", "value": "us-east-2" },
                { "label": "us-west-1", "value": "us-west-1" },
                { "label": "us-west-2", "value": "us-west-2" },
                { "label": "eu-central-1", "value": "eu-central-1" },
                { "label": "eu-north-1", "value": "eu-north-1" },
                { "label": "eu-west-1", "value": "eu-west-1" },
                { "label": "eu-west-2", "value": "eu-west-2" },
                { "label": "eu-west-3", "value": "eu-west-3" },
                { "label": "sa-east-1", "value": "sa-east-1" },
                { "label": "ap-northeast-1", "value": "ap-northeast-1" },
                { "label": "ap-northeast-2", "value": "ap-northeast-2" },
                { "label": "ap-northeast-3", "value": "ap-northeast-3" },
                { "label": "ap-south-1", "value": "ap-south-1" },
                { "label": "ap-southeast-1", "value": "ap-southeast-1" },
                { "label": "ap-southeast-2", "value": "ap-southeast-2" },
                { "label": "ca-central-1", "value": "ca-central-1" }
              ]
            },
            {
              "type": "text",
              "name": "stackName",
              "label": "Enter your stack name",
              "required": true,
              "description": "The name of your stack",
              "value": "logzio-cloudwatch-shipper",
              "pattern": "^[a-zA-Z0-9-]+$",
              "patternError": "Only letters, numbers and dash"
            }
             ],
           "telemetries": [
             {
               "type": "LOG_ANALYTICS",
               "hint": "Switch the toggle on if you want Logz.io Telemetry Collector to collect logs from your EC2.",
               "params":
               [
                {
                  "type": "boolean",
                  "name": "param_logzioCompress",
                  "label": "Compress",
                  "description": "Send logs compressed",
                  "value": true
                },
                {
                  "type": "key-value",
                  "name": "param_logzioEnrich",
                  "label": "Logzio Enrich",
                  "description": "Key value param description"
                },
                {
                  "type": "single-select",
                  "name": "param_logzioFormat",
                  "label": "Logzio Format",
                  "description": "format",
                  "options": [
                    { "label": "text", "value": "text" },
                    { "label": "json", "value": "json" }
                  ],
                  "value": "text"
                },
                {
                  "type": "boolean",
                  "name": "param_logzioSendAll",
                  "label": "LogzioSendAll",
                  "description": "LogzioSendAll",
                  "value": false
                },
                {
                  "type": "text",
                  "name": "param_logzioType",
                  "label": "Log Type",
                  "required": true,
                  "description": "Log type",
                  "value": "logzio-cloudwatch-lambda",
                  "pattern": "^[a-zA-Z0-9-]+$",
                  "patternError": "Only letters, numbers and dash"
                },
                {
                  "type": "number",
                  "name": "param_shipperLambdaTimeout",
                  "label": "shipper lambda timeout",
                  "description": "The number of seconds that Lambda allows a function to run before stopping it, for the shipper function.",
                  "value": 60
                },
                {
                  "type": "number",
                  "name": "param_shipperLambdaMemory",
                  "label": "shipper lambda memory",
                  "description": "Shipper function's allocated CPU proportional to the memory configured, in MB.",
                  "value": 512
                },
                {
                  "type": "multi-select",
                  "name": "param_services",
                  "label": "services",
                  "description": "Services you want to collect logs from",
                  "options": [
                    { "label": "API Gateway", "value": "apigateway" },
                    { "label": "RDS", "value": "rds" },
                    { "label": "CloudHSM", "value": "cloudhsm" },
                    { "label": "CloudTrail", "value": "cloudtrail" },
                    { "label": "CodeBuild", "value": "codebuild" },
                    { "label": "Amazon Connect", "value": "connect" },
                    { "label": "AWS Elastic Beanstalk", "value": "elasticbeanstalk" },
                    { "label": "ECS", "value": "ecs" },
                    { "label": "EKS", "value": "eks" },
                    { "label": "AWS Glue", "value": "aws-glue" },
                    { "label": "AWS IoT", "value": "aws-iot" },
                    { "label": "Lambda", "value": "lambda" },
                    { "label": "Amazon Macie", "value": "macie" },
                    { "label": "Amazon MQ", "value": "amazon-mq" }
                  ]
                },
                { 
                  "type": "path-list",
                  "name": "param_customLogGroups",
                  "label": "custom log groups",
                  "description": "custom log groups you want to collect logs from"
                },
                {
                  "type": "number",
                  "name": "param_triggerLambdaTimeout",
                  "label": "trigger lambda timeout",
                  "description": "The number of seconds that Lambda allows a function to run before stopping it, for the trigger function.",
                  "value": 60
                },
                {
                  "type": "number",
                  "name": "param_triggerLambdaMemory",
                  "label": "trigger lambda memory",
                  "description": "Trigger function's allocated CPU proportional to the memory configured, in MB.",
                  "value": 512
                },
                {
                  "type": "single-select",
                  "name": "param_triggerLambdaLogLevel",
                  "label": "trigger lambda log level",
                  "description": "Log level for the trigger function",
                  "options": [
                    { "label": "debug", "value": "debug" },
                    { "label": "info", "value": "info" },
                    { "label": "warn", "value": "warn" },
                    { "label": "error", "value": "error" },
                    { "label": "fatal", "value": "fatal" },
                    { "label": "panic", "value": "panic" }
                  ],
                  "value": "info"
                }
               ]
             }
           ]
         }
       ]
     }
   ]
 }