{
    "commands": [
        {
            "description": "Is Kubectl installed?",
            "run": [
                "which kubectl >/dev/null 2>&1;",
                "if [ $? -ne 0 ]; then", 
                    "echo \"Kubectl is not installed\";",
                    "exit 1;",
                "else",
                    "echo \"Kubectl is already installed\";",
                "fi;"
            ] 
        }
    ]
}